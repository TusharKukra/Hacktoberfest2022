#include <bits/stdc++.h>
using namespace std;
//Code by Abhimanyu
void nP(vector<int>& nums) {
        int n = nums.size(), k, l;
    	for (k = n - 2; k >= 0; k--) {
            if (nums[k] < nums[k + 1]) {
                break;
            }
        }
    	if (k < 0) {
    	    reverse(nums.begin(), nums.end());
    	} else {
    	    for (l = n - 1; l > k; l--) {
                if (nums[l] > nums[k]) {
                    break;
                }
            } 
    	    swap(nums[k], nums[l]);
    	    reverse(nums.begin() + k + 1, nums.end());
        }
    }
int main()
{
     vector<int>ans{1,3,2,5};
	nP(ans);
	cout<<ans[0]<<" "<<ans[1]<<" "<<ans[2]<<" "<<ans[3];
	return 0;
}


